# Database Configuration
DATABASE_URL=postgres://username:password@localhost:5432/dbname?sslmode=disable

# JWT Secret (change this in production)
JWT_SECRET=your-secret-key

# Google OAuth Client ID (for verifying Google ID tokens)
GOOGLE_CLIENT_ID=your-google-client-id

# Groq(get from https://console.groq.com/keys)
GROQ_API_KEY=

# Tavily (get from https://tavily.com/)
TAVILY_API_KEY=

# SerpApi (get from https://serpapi.com/)
SERPAPI_API_KEY=

# Feed API Key (any secret string for REST API authentication)
FEED_API_KEY=your-secret-api-key

# Razorpay Credentials (get from https://dashboard.razorpay.com/app/keys)

RAZORPAY_KEY_ID=
RAZORPAY_KEY_SECRET=
RAZORPAY_WEBHOOK_SECRET=
# Options: "popup" (default) or "redirect"
RAZORPAY_PAYMENT_FLOW=popup